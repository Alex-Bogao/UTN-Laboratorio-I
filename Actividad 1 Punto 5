#include <iostream>
using namespace std;
int main () {
    float importe , porcentaje , descuento , descuento2 ;
    cout << "ingrese importe : " ;
    cin >> importe ;
    cout << "ingrese porcentaje de descuento : " ;
    cin >> porcentaje ;
    descuento = (porcentaje / 100) * importe ;
    descuento2 = importe - descuento ;
    cout << "tu descuento es : " << descuento2 ;

       return 0 ;
}
